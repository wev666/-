Sectionhuanjin:NewToggle("删除岩浆", "把岩浆删除",  function(state)
	for i,v in pairs(game.Workspace.Region_Volcano:GetDescendants()) do
		if v.Name == "TouchInterest" then
		  v:Destroy()
		elseif v.Name == "Lava" then
		  for n,k in pairs(v:GetChildren()) do
			if k:IsA("Part") then
			  if state == true then
				k.Transparency = 1
			  else
				k.Transparency = 0
			  end
			end
		  end
		end
	  end

end)
