			StopSpectating();
	game:GetService('Workspace').CurrentCamera.CameraSubject=bai.Players:FindFirstChild(bai.playernamedied).Character.Humanoid
bai.connections.spectateAdded=bai.Players:FindFirstChild(bai.playernamedied).CharacterAdded:Connect(function()
	repeat wait()until bai.Players:FindFirstChild(bai.playernamedied):FindFirstChild'Humanoid'
	game:GetService('Workspace').CurrentCamera.CameraSubject=bai.Players:FindFirstChild(bai.playernamedied).Character.Humanoid




end)
bai.connections.spectateAddedLocal=lp.CharacterAdded:Connect(function()
	repeat wait()until game:GetService('Workspace').CurrentCamera.CameraSubject==lp.Character:WaitForChild'Humanoid'
	game:GetService('Workspace').CurrentCamera.CameraSubject=bai.Players:FindFirstChild(bai.playernamedied).Character.Humanoid
end);
bai.connections.playerLeft=game:GetService'Players'.ChildRemoved:Connect(function(Player)
	if tostring(Player)==tostring(bai.playernamedied)then 
		StopSpectating();
	end;
end);
	end)
