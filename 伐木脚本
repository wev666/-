	Section3:NewToggle("穿墙", "穿过墙壁", function(state)
		if state then 
			bai.donoclip = true
			bai.noclip=true
			lp.Character.Head.Touched:connect(function(obj)
				if obj ~= workspace.Terrain then
				if bai.donoclip == true then
				obj.CanCollide = false
				else
				obj.CanCollide = true
				end
				end
			end)
	else bai.donoclip = false

		bai.noclip = false
		end
		
	end)
