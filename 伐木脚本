BuyItem = function(m)
    return b.NPCDialog.PlayerChatted:InvokeServer(m, "ConfirmPurchase")
end
local n = GetShopItems("BasicHatchet")
for e, h in next, n:children() do
    if not table.find(l, h.BoxItemName.Value) then
        table.insert(l, h.BoxItemName.Value)
    end
end
function WoodRus_AutoBuy_Ltem(o)
    local p = c.Character.HumanoidRootPart.CFrame
    local q
    for e, h in next, a.Stores:children() do
        if h.Name == "ShopItems" and h:FindFirstChild "Box" then
            for i, j in next, h:children() do
                if j.BoxItemName.Value == o then
                    ltem = j
                end
            end
        end
    end
    Teleport(ltem.Main.CFrame + Vector3.new(0, 1, 5))
    task.wait()
    for e = 1, 5 do
        DragModel(ltem, a.Stores.WoodRUs.Counter.CFrame + Vector3.new(0, .6, 0))
        Teleport(a.Stores.WoodRUs.Counter.CFrame + Vector3.new(0, 5, 5))
        task.wait(.01)
    end
    task.wait(.5)
    spawn(
        function()
            repeat
                task.wait(.01)
                BuyItem(GetShopID.WoodRus)
            until ltem.Owner.Value == c
        end
    )
    task.wait(.5)
    for e = 1, 30 do
        DragModel(ltem, p)
        task.wait(.01)
    end
    Teleport(p)
    return o
end
function WoodRus_AutoBuy(o, r)
    for e = 1, r do
        WoodRus_AutoBuy_Ltem(o)
        task.wait()
    end
    return o, r
end
